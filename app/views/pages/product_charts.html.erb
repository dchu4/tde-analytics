<div ng-app="app">
  <div ng-controller="chartsCtrl" ng-init="productsSetup()">

    <div class="row">
      <div class="col s12 m12 l12">
        <div class="card">
          <div class="section-card-title">
            <span class="card-title">Visits and Purchases in the Last 30 Days</span>
          </div>
          <div class="card-content">
            <canvas id="month_timeline_chart" height="100"></canvas>
          </div>
        </div>
      </div>
    </div>

    <div class="row">
      <div class="col s12 m12 l12">
        <div class="card">
          <div class="section-card-title">
            <span class="card-title">Visits and Purchases Per Product</span>
          </div>
          <div class="card-content">
            <canvas id="visit_purchase_chart" height="100"></canvas>
          </div>
        </div>
      </div>
    </div>

  </div>
</div>