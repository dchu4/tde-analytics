<div class="row">
  <div class="col s12 m3 l3">
    <div class="card-panel">
      <div class="center-align">Average Visit Duration</div>
      <h5 class="center-align"><%= @avg_visit_duration %></h5>
    </div>
  </div>
</div>

<div ng-app="app">
  <div ng-controller="visitsCtrl" ng-init="visitsSetup()">

    <div class="row">
      <div class="col s12 m12 l12">
        <div class="card">
          <div class="section-card-title">
            <span class="card-title">Visits and Purchases in the Last 30 Days</span>
          </div>
          <div class="card-content">
            <canvas id="month_timeline_chart" height="100"></canvas>
          </div>
        </div>
      </div>
    </div>

  </div>
</div>