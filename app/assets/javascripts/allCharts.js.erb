$(document).ready(function() {

  $.getJSON('pages/visits_timeline_chart', function(result) {
    var chartData = {
      labels: result["dates"],
      datasets: [{
        label: 'Number of Visits',
        data: result["numbers"],
        backgroundColor: [
          'rgba(255, 99, 132, 0.2)'
        ],
        borderColor: [
          'rgba(255,99,132,1)'
        ],
        borderWidth: 1
      }]
    };

    var vtc = document.getElementById("visits_timeline_chart");

    var visits_timeline_chart = new Chart(vtc, {
      type: 'line',
      data: chartData
    });

  });

});